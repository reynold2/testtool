<?xml version="1.0" encoding="utf-8"?>
<Plugin Filename="tech.microcore.service.dynamiclayout" id="tech.microcore.service.dynamiclayout">
    <ExtPointSet>
        <ExtPoint name="mainwindow dynamiclayout" displaylabel="title" description="" id="tech.microcore.mainwin.core.dynamiclayout" displayicon="icon" hasformat="">
            <String name="" regexp="[a-zA-Z][a-zA-Z0-9\-_.]*" trans="true" tips="" key="key" value="DynamicLayout"/>
            <String name="" regexp="" trans="true" tips="" key="title" value=""/>
            <Bool name="" key="isFactory" value="true"/>
            <File name="" filter="Image Files (*.png)" method="getOpenFileName" key="icon" title="Select Icon File..."/>
        </ExtPoint>
        <ExtPoint name="mainwindow dynamiclayout style" displaylabel="title" description="" id="tech.microcore.mainwin.core.dynamiclayout.style" displayicon="icon" hasformat="">
            <String name="" regexp="[a-zA-Z][a-zA-Z0-9\-_.]*" trans="true" tips="" key="key" value="StyleWidget"/>
            <String name="" regexp="" trans="true" tips="" key="title" value=""/>
            <Bool name="" key="isFactory" value="true"/>
            <File name="" filter="Image Files (*.png)" method="getOpenFileName" key="icon" title="Select Icon File..."/>
        </ExtPoint>
    </ExtPointSet>
    <ExtDataSet>
        <ExtData id="tech.microcore.mainwin.core.control" extPlugin="tech.microcore.mainwin.core" plugin="tech.microcore.service.dynamiclayout">
            <Format target="tech.microcore.mainwin.desktop">
                <Action id="tech.microcore.service.dynamiclayout.edit" parent="tech.microcore.mainwin.desktop.toolbarPic"/>
            </Format>
            <Action checked="false" text="tr('LayoutEdit')" toolTip="tr('layout edit')" id="tech.microcore.service.dynamiclayout.edit" visible="true" icon="images/puzzle.png" checkable="true" sceneMode="true" enabled="true" autoRepeat="false"/>
        </ExtData>
        <ExtData DefAreas="2" id="tech.microcore.mainwin.core.dockview" allowedAreas="15" visible="true" extPlugin="tech.microcore.mainwin.core" isFloating="false" plugin="tech.microcore.service.dynamiclayout">
            <dockview DefAreas="Left" id="tech.microcore.service.dynamiclayout.contenttree" allowedAreas="15" visible="true" isFloating="false" title="tr('ContentTree')"/>
        </ExtData>
    </ExtDataSet>
</Plugin>
